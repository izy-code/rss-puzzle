{
  "name": "word-puzzle",
  "version": "1.0.0",
  "description": "RSS word puzzle task",
  "main": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsx node_modules/webpack/bin/webpack --mode=production --node-env=production",
    "build:dev": "tsx node_modules/webpack/bin/webpack --mode=development --node-env=development",
    "build:prod": "tsx node_modules/webpack/bin/webpack --mode=production --node-env=production",
    "deploy": "npm run build && npx gh-pages -d dist -e news-api",
    "format": "npx prettier --write .",
    "ci:format": "npx prettier --check .",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint --fix ./src",
    "typecheck": "tsc",
    "watch": "tsx node_modules/webpack/bin/webpack --mode=development --node-env=development --watch",
    "start": "tsx node_modules/webpack/bin/webpack serve --mode=development --node-env=development",
    "prepare": "cd .. && husky rss-puzzle/.husky"
  },
  "contributors": [
    "Sergey Koksharov <gentoosiastic@gmail.com>"
  ],
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv-webpack": "^7.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@webpack-cli/generators": "^3.0.4",
    "autoprefixer": "^10.4.18",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "esbuild-loader": "^4.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.25.2",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "gh-pages": "^6.0.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^9.0.11",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.3",
    "stylelint": "15.11.0",
    "stylelint-config-standard-scss": "11.1.0",
    "stylelint-order": "6.0.3",
    "ts-loader": "^9.4.3",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.4",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^5.0.1"
  }
}
